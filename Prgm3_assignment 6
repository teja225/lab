class Node: 

	def __init__(self): 
		self.data = None
		self.next = None

head = None

def largestElement(head): 

	max = -32767

	while (head != None): 

		if (max < head.data) : 
			max = head.data 
		head = head.next

	return max

def smallestElement(head): 

	min = 32767 
	while (head != None) : 

		if (min > head.data) : 
			min = head.data 
		head = head.next

	return min

def push( data) : 

	global head 

	newNode = Node()  
	newNode.data = data 

	newNode.next = (head)  
	(head) = newNode 

def printList( head) : 

	while (head != None) : 
		print(head.data ,end= " -> ") 
		head = head.next

	print("None")  

push( 15) 
push( 14) 
push( 13) 
push( 22) 
push( 17) 
print("Linked list is : ") 

printList(head) 
print("Maximum element in linked list: ",end="") 
print(largestElement(head)) 
print("Minimum element in linked list: ",end="")  
print(smallestElement(head),end="") 

OUTPUT:
Linked list is : 
17 -> 22 -> 13 -> 14 -> 15 -> None
Maximum element in linked list: 22
Minimum element in linked list: 13
